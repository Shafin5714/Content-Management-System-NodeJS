  <script>
  $(document).ready(function(){
        setTimeout(function(){
          $('.auto-hide').slideUp(300)
        },1000)
    })
  
$(function(){
  $('#searchName').autocomplete({
    source:function(req,res){
      $.ajax({
        url:"/autocomplete",
        dataType:"jsonp",
        GET:"GET",
        data:req,
        success: function(data){
          res(data)
        },
        error:function(err){
          console.log(err.status)
        }

      })
    },
    minLength:1,
    select:function(event,ui){
      if(ui.items){
        $('#searchName').text(ui.item.label)
      }
    }
  })
})

  </script>
 

